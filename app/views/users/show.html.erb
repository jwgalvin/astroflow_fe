<a id="button" class="button-left"><%= button_to "Edit profile here", "/register", method: :get %></a>
<br><h1 id="dashboard-welcome"> Welcome <%= @user.name %>! </h1>
<div id="dashboard" class="dash-main">

  <div id="personal-info" class="container text_color display-padding2">
    <h3>Daily Horoscope</h3>
    <h4><%= @today_horoscope.current_date %></h4>
    <br>
   <%= @today_horoscope.description %>
   <br>
   <br>
   <h3>Today's Vibes</h3>
   <h4>Color: <%= @today_horoscope.color %></h4>
   <h4>Comptaibility: <%= @today_horoscope.compatibility %></h4>
   <h4>Lucky Number: <%= @today_horoscope.lucky_number %></h4>
   <h4>Lucky Time: <%= @today_horoscope.lucky_time %></h4>
  
   <h3>Moon</h3>
   <h4>Moon: <%= @today_horoscope.moon %></h4>
   <h4>Moon: <%= @today_horoscope.moon_phase %></h4>


  </div>
  <div id="horoscope-stuff" class="container text_color display-padding">
   <h3>Yesterday's Horoscope</h3>
    <h4><%= @yesterday_horoscope.current_date %></h4>
    <br>
   <%= @yesterday_horoscope.description %>
   <br>
   <br>
   <h3>Yesterday's Vibes</h3>
   <h4>Color: <%= @yesterday_horoscope.color %></h4>
   <h4>Comptaibility: <%= @yesterday_horoscope.compatibility %></h4>
   <h4>Lucky Number: <%= @yesterday_horoscope.lucky_number %></h4>
   <h4>Lucky Time: <%= @yesterday_horoscope.lucky_time %></h4>
  
   <h3>Moon</h3>
   <h4>Moon: <%= @yesterday_horoscope.moon %></h4>
   <h4>Moon: <%= @yesterday_horoscope.moon_phase %></h4>
  </div>
</div>
